        <ownedAttribute xmi:id="_Y-qHcUa5EeqM2oA8zDGwTg" name="points" visibility="private" type="_Y75HUUa5EeqM2oA8zDGwTg" association="_ZBOTQUa5EeqM2oA8zDGwTg"/>
        <ownedAttribute xmi:id="_Y-wOEEa5EeqM2oA8zDGwTg" name="position" isReadOnly="true">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
