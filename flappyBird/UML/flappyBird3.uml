        </ownedAttribute>
        <ownedAttribute xmi:id="_Y-yDQEa5EeqM2oA8zDGwTg" name="yPrev" visibility="private">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
        </ownedAttribute>
        <ownedAttribute xmi:id="_Y-z4cUa5EeqM2oA8zDGwTg" name="firstPoint" visibility="private">
