        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_ZAQC4Ea5EeqM2oA8zDGwTg" name="moveDown" specification="_YwcFQEa5EeqM2oA8zDGwTg">
          <ownedParameter xmi:id="_ZAQC4Ua5EeqM2oA8zDGwTg" name="chute">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          </ownedParameter>
          <language>java</language>
          <body>if(this.hauteur+chute+Affichage.ovalHeight &lt; Affichage.HAUT &amp;&amp; !testPerdu()) {
		        	this.hauteur += chute;
		     }
		 </body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_ZAQC4ka5EeqM2oA8zDGwTg" name="testPerdu" specification="_YwcsUEa5EeqM2oA8zDGwTg">
          <ownedParameter xmi:id="_ZAQC40a5EeqM2oA8zDGwTg" direction="return">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
          </ownedParameter>
          <language>java</language>
          <body>Point[] points = this.parcours.getParcours();
			 int i=0;
			 Point A = points[i];
			 Point B = points[i+1];
			 
			 //pas de game over si on a pas encore atteint le début de la courbe
			 if(A.x>=Affichage.ovalDec) {
				 return false;
			 }else {
				 
				 
				 //tant que le deuxième point sélectioné est à gauche du milieu de l'ovale, on décal la sélection de 1 point 
				 while(B.x &lt; Affichage.ovalDec) {
					 i++;
					 A = points[i];
					 B = points[i+1];
				 }
				float coeffDir = floatDiv(B.y - A.y ,(B.x - A.x));
			 
			 	float Ypos0 = ((-A.x+Affichage.ovalDec) * coeffDir) + A.y;
			 	this.yPos0 = Ypos0;
			 	return this.hauteur + Affichage.largTraits.getLineWidth() >= Ypos0 || this.hauteur+Affichage.ovalHeight - Affichage.largTraits.getLineWidth() &lt;= Ypos0;
			 }
		 </body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_ZAQp8Ea5EeqM2oA8zDGwTg" name="floatDiv" specification="_YwdTYEa5EeqM2oA8zDGwTg">
