        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_ZAO0wEa5EeqM2oA8zDGwTg" name="jump" specification="_YwbeMEa5EeqM2oA8zDGwTg">
          <language>java</language>
          <body>if(!testPerdu()) {
			 	if(this.hauteur-Etat.saut > 0) {
				 	this.hauteur -= Etat.saut;
		     	}else {
		    	 	this.hauteur = 0;
