          <language>java</language>
          <body>Point[] p = this.etat.getParcours().getParcours();
     	
     	int i=1;
     	while(i &lt; p.length) {
     		g.drawLine(p[i-1].x, p[i-1].y, p[i].x, p[i].y);
     		i++;
     	}
     </body>
